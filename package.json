{
  "name": "vue-chevron",
  "version": "0.0.1",
  "description": "Animated chevron toggle component",
  "repository": {
    "url": "ispal/vue-chevron",
    "type": "git"
  },
  "main": "dist/vue-chevron.common.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepublishOnly": "npm test && npm run build",
    "test": "test-vue-app",
    "build": "bili --compress umd",
    "example": "poi",
    "build:example": "poi build",
    "release": "np"
  },
  "author": {
    "name": "Irko. Palenius",
    "email": "ispal84@gmail.com"
  },
  "license": "MIT",
  "poi": {
    "entry": "example/index.js",
    "dist": "example/dist",
    "homepage": "./"
  },
  "bili": {
    "format": [
      "cjs",
      "umd",
      "es"
    ],
    "plugins": [
      "vue"
    ],
    "vue": {
      "css": "dist/vue-chevron.css"
    }
  },
  "devDependencies": {
    "bili": "^0.18.2",
    "eases": "^1.0.8",
    "node-sass": "^4.6.1",
    "normalize.css": "^7.0.0",
    "np": "^2.16.1",
    "poi": "^9.3.10",
    "prismjs": "^1.8.4",
    "rollup-plugin-vue": "^2.5.2",
    "sass-loader": "^6.0.6",
    "test-vue-app": "^1.0.0",
    "vue-switches": "^2.0.1",
    "vue-test-utils": "^1.0.0-beta.2"
  },
  "dependencies": {
    "vue-prism-component": "^1.0.1"
  }
}
