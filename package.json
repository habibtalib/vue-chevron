{
  "name": "vue-chevron",
  "version": "0.0.1",
  "description": "Animated chevron toggle component",
  "repository": {
    "url": "https://github.com/ispal/vue-chevron",
    "type": "git"
  },
  "main": "dist/vue-chevron.common.js",
  "module": "dist/vue-chevron.es.js",
  "files": ["dist"],
  "release": {
    "branchTags": {
      "develop": "dev"
    },
    "fallbackTags": {
      "master": "latest"
    },
    "analyzeCommits": "@telerik/semantic-prerelease/analyzeCommits",
    "generateNotes": "@telerik/semantic-prerelease/generateNotes",
    "getLastRelease": "@telerik/semantic-prerelease/getLastRelease",
    "verifyConditions": "@telerik/semantic-prerelease/verifyConditions",
    "verifyRelease": "@telerik/semantic-prerelease/verifyRelease"
  },
  "scripts": {
    "prepublishOnly": "npm test && npm run build",
    "test": "test-vue-app",
    "build": "bili --compress umd",
    "example": "poi",
    "build:example": "poi build",
    "commit": "git-cz",
    "semantic-release":
      "semantic-release pre && semantic-prerelease publish && semantic-release post"
  },
  "author": {
    "name": "Ispal",
    "email": "irpa.oss@gmail.com"
  },
  "license": "MIT",
  "poi": {
    "entry": "example/index.js",
    "dist": "docs",
    "homepage": "./"
  },
  "bili": {
    "format": ["cjs", "umd", "es"],
    "plugins": ["vue"],
    "vue": {
      "css": "dist/vue-chevron.css"
    }
  },
  "devDependencies": {
    "@telerik/semantic-prerelease": "1.2.1",
    "bili": "0.18.2",
    "commitizen": "2.9.6",
    "cz-conventional-changelog": "2.1.0",
    "eases": "1.0.8",
    "gh-pages": "1.0.0",
    "node-sass": "4.6.1",
    "normalize.css": "7.0.0",
    "poi": "9.5.2",
    "prismjs": "1.8.4",
    "rollup-plugin-vue": "2.5.2",
    "sass-loader": "6.0.6",
    "semantic-release": "8.2.0",
    "test-vue-app": "1.0.0",
    "vue-prism-component": "1.0.1",
    "vue-switches": "2.0.1",
    "vue-test-utils": "1.0.0-beta.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
